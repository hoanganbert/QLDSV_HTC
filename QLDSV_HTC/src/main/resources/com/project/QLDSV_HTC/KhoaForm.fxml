<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.QLDSV_HTC.controller.SecondaryController">
    <AnchorPane>
	    <TableView fx:id="khoaTable">
	        <columns>
	            <TableColumn fx:id="maKhoaColumn" text="Mã Khoa" prefWidth="100"/>
	            <TableColumn fx:id="tenKhoaColumn" text="Tên Khoa" prefWidth="150"/>
	        </columns>
	    </TableView>
	</AnchorPane>

    <children>
        <Button fx:id="secondaryButton" text="Switch to Primary View" onAction="#switchToPrimary" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
